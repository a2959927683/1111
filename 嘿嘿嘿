v1
#include <stdio.h>
int main()
{
	char str[]="3+4";
	int a=str[0]-'0';
	int b=str[2]-'0';
	int total = a + b;
	printf("total=%d\n",total);
	return 0;
}








v2
#include <stdio.h>
#include <string.h>
int main()
{
	char str[]="1+2+2+1+2+5+4-1-3+4-8";
	int sum=str[0]-'0';
	for(int i=1;i<strlen(str);i=i++)
	{
		if(str[i]=='+')
		{
			int a=str[i+1]-'0';
		    sum=sum+a;
			i++;
		}
		else if(str[i]=='-')
		{
		    int b=str[i+1]-'0';
		    sum=sum-b;
			i++;
		}
	}
	printf("sum=%d\n",sum);
	return 0;
}










v3
#include<stdio.h>
#include<string.h>
int main()
{	
	char a[]="2+2*3+2/2-1";
	char b[1000];
	int i;
	int y=-1;
	for(i=0;i<strlen(a);i++)
	{	
		if(a[i]=='*')
		{
			int x=b[y]-'0';
			int z=a[i+1]-'0';
			b[y]=x*z+'0';
			i++;
		}
		else if(a[i]=='/')
		{
			int x=b[y]-'0';
			int z=a[i+1]-'0';
			b[y]=x/z+'0';
			i++;
		}
	else b[++y]=a[i];
	}
	int res=b[0]-'0';
	for(i=0;i<strlen(a);i++)
	{
		if(b[i]=='+') res+=b[++i]-'0';
		else if(b[i]=='-') res-=b[++i]-'0';
	}
printf("res=%d\n",res);
return 0;
}
